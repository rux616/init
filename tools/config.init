backup {{home_dir}}/.local/bin/kubectx
backup {{home_dir}}/.local/bin/kubens
backup {{home_dir}}/.local/share/bash-completion/completions/kubectx
backup {{home_dir}}/.local/share/bash-completion/completions/kubens
backup {{home_dir}}/.local/share/bash-completion/completions/tmux
backup {{home_dir}}/.local/share/bash-completion/completions/packer
backup {{home_dir}}/.local/share/bash-completion/completions/terraform

submodule {{config_dir}}/kubectx
submodule {{config_dir}}/tmux-bash-completion

directory {{home_dir}}/.local/bin
directory {{home_dir}}/.local/share/bash-completion/completions

link {{config_dir}}/kubectx/kubectx {{home_dir}}/.local/bin/kubectx
link {{config_dir}}/kubectx/kubens {{home_dir}}/.local/bin/kubens
link {{config_dir}}/kubectx/completions/kubectx.bash {{home_dir}}/.local/share/bash-completion/completions/kubectx
link {{config_dir}}/kubectx/completions/kubens.bash {{home_dir}}/.local/share/bash-completion/completions/kubens
link {{config_dir}}/tmux-bash-completion/completions/tmux {{home_dir}}/.local/share/bash-completion/completions/tmux
link {{config_dir}}/completions/packer {{home_dir}}/.local/share/bash-completion/completions/packer
link {{config_dir}}/completions/terraform {{home_dir}}/.local/share/bash-completion/completions/terraform
